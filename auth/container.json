{
    "id": "auth",
    "type": "wedeploy/auth",
    "env": {
        "WEDEPLOY_AUTH_SECURE_FIELDS": "password",
        "WEDEPLOY_AUTH_PASSWORD": "true",
        "WEDEPLOY_AUTH_GOOGLE": "true",
        "WEDEPLOY_AUTH_GOOGLE_CLIENT_ID": "<your-google-app-id>",
        "WEDEPLOY_AUTH_GOOGLE_CLIENT_SECRET": "<your-google-app-secret>"
    }
}